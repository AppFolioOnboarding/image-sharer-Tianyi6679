<h1 id="page-header">
  All Images
</h1>
<%= link_to "Add your own images", new_image_path %>
<table id="js-image-list">
  <tr>
    <th>Title</th>
    <th>Preview</th>
  </tr>

  <% @images.reverse_each do |image| %>
    <tr class="js-image">
      <td><%= image.title %></td>
      <td><%= link_to(image_tag(image.url, size: 400), image_path(image)) %></td>
    </tr>
  <% end %>
</table>
